{
  "name": "@bya2/rusty-container-api",
  "version": "1.2.1",
  "description": "Use rust-like optional and error-handling types API.",
  "bugs": "https://github.com/bya2/rusty-container-api/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bya2/rusty-container-api.git"
  },
  "license": "MIT",
  "author": "Jinhyeong Lee <byaa1972@gmail.com> (https://github.com/bya2)",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "compile": "tsc",
    "test": "jest -o",
    "test:all": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll"
  },
  "dependencies": {
    "lodash.isequal": "4.5.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.12.7",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "ts-jest": "29.1.2",
    "tsup": "8.0.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.9.0"
}
