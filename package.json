{
  "name": "@bya2/js-option",
  "version": "1.0.7",
  "description": "Rust의 기능인 `Option`를 Javascript에서 사용합니다.",
  "author": "Jinhyeong Lee <byaa1972@gmail.com>",
  "repository": "https://github.com/bya2/js-option.git",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "test": "jest -o",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "build": "tsup",
    "compile": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/lodash.isequal": "^4",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@bya2/js-result": "^1.0.6",
    "lodash.isequal": "^4.5.0"
  }
}
